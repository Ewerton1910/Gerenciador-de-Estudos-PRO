{
  "builds": [
    {
      "src": "middleware.js",
      "use": "@vercel/edge"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "middlewarePath": "middleware",
      "continue": true
    }
  ]
}
